{
  "name": "steamladder",
  "version": "2.0.2",
  "description": "Node.js wrapper for the Steam Ladder API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/**/*.ts"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "steam",
    "steamladder",
    "api",
    "wrapper"
  ],
  "author": "Switchblade Team <support@switchblade.xyz>",
  "license": "MIT",
  "homepage": "https://github.com/SwitchbladeBot/steamladder.js#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SwitchbladeBot/steamladder.js.git"
  },
  "bugs": {
    "url": "https://github.com/SwitchbladeBot/steamladder.js/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "axios": "^0.23.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "tsup": "^5.7.2",
    "typescript": "^4.4.4"
  }
